{
  "name": "sauls/collections",
  "description": "Sauls collections",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Saulius Vaičeliūnas",
      "email": "saulius@vaiceliunas.lt"
    }
  ],
  "require": {
    "php": ">=7.2",
    "sauls/helpers": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Sauls\\Component\\Collection\\": "src/"
    },
    "exclude-from-classmap": [
